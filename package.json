{
  "name": "chrome-socket-request",
  "version": "0.0.4",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Wrapper element for the SocketFetch libraty to make a HTTP connection in Chrome",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "private": false,
  "main": [
    "chrome-socket-request.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint chrome-socket-request.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"chrome-socket-request:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/chrome-socket-request.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/chrome-socket-request/issues",
    "email": "arc@mulesoft.com"
  }
}